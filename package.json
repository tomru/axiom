{
  "name": "bw-axiom",
  "description": "",
  "version": "0.0.8",
  "main": "./lib",
  "license": "MIT",
  "homepage": "http://axiom.brandwatch.com/",
  "bugs": {
    "url": "https://github.com/BrandwatchLtd/axiom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrandwatchLtd/axiom.git"
  },
  "scripts": {
    "clean": "babel-node -e \"require('./scripts/clean').default()\"",
    "build-client": "NODE_ENV=production BABEL_ENV=production babel-node -e \"require('./scripts/build-client').default()\"",
    "build-server": "NODE_ENV=production BABEL_ENV=production babel-node -e \"require('./scripts/build-server').default()\"",
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "node_modules/.bin/eslint --config .eslintrc ./src ./style-guide",
    "lint-sass": "node_modules/.bin/sass-lint --config .sass-lint.yml -v",
    "start": "NODE_ENV=development BABEL_ENV=development babel-node -e \"require('./scripts/develop').default()\"",
    "prod": "NODE_ENV=production BABEL_ENV=production babel-node -e \"require('./scripts/production').default()\"",
    "prepublish": "npm run clean && npm run prepublish-javascript && npm run prepublish-sass",
    "prepublish-javascript": "BABEL_ENV=production babel ./src --out-dir ./lib && babel ./utils --out-dir ./lib/utils",
    "prepublish-sass": "NODE_ENV=production BABEL_ENV=production babel-node -e \"require('./scripts/prepublish-sass').default()\"",
    "test": "mocha $( find ./src ./utils ./style-guide -name '*.tests.js') ${exeparams[@]}"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "classnames": "^2.2.1",
    "font-awesome": "^4.5.0",
    "moment": "^2.14.1",
    "normalize.css": "^4.2.0",
    "numbro": "^1.9.2",
    "react": "^15.3.0",
    "react-modal": "^1.4.0",
    "react-router": "2.6.1"
  },
  "peerDependencies": {
    "react": "^15.3.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "^0.23.1",
    "decamelize": "^1.2.0",
    "del": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "forever": "0.15.2",
    "fs-extra": "^0.30.0",
    "fuse.js": "^2.4.1",
    "glob": "^7.0.3",
    "humanize-string": "^1.0.1",
    "js-beautify": "^1.6.2",
    "jsdom": "^9.4.1",
    "json-loader": "0.5.4",
    "loader-utils": "^0.2.14",
    "lodash": "^4.14.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "prismjs": "^1.4.1",
    "react-dom": "^15.3.0",
    "react-element-to-jsx-string": "^4.2.0",
    "react-redux": "^4.0.0",
