{
  "name": "bw-axiom",
  "description": "Axiom - Brandwatch Pattern Library",
  "version": "0.0.0-development",
  "main": "./lib",
  "license": "MIT",
  "homepage": "https://BrandwatchLtd.github.io/axiom",
  "bugs": {
    "url": "https://github.com/BrandwatchLtd/axiom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrandwatchLtd/axiom.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build-css": "NODE_ENV=production BABEL_ENV=production webpack --config webpack.css.config.js",
    "build-static": "NODE_ENV=production BABEL_ENV=production webpack --config webpack.static.config.js",
    "gh-pages": "BASENAME_ENV=\"'/axiom/'\" yarn build-static && gh-pages -d ./static --repo git@github.com:BrandwatchLtd/axiom.git",
    "lint": "yarn run lint-js && yarn run lint-css",
    "lint-css": "stylelint 'src/**/*.css' 'style-guide/**/*.css'",
    "lint-js": "eslint --config .eslintrc ./src ./style-guide",
    "prepublish": "rm -rf ./lib && yarn prepublish-js && yarn prepublish-css",
    "prepublish-css": "rsync -a --prune-empty-dirs --exclude 'example/*.css' --include '*/' --include '*.css' --include 'theme-*.json' --exclude '*' ./src/* ./lib",
    "prepublish-js": "NODE_ENV=production BABEL_ENV=production babel ./src --ignore example,*.test.js --out-dir ./lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "webpack-dev-server --port 4000 --inline --hot --history-api-fallback --content-base style-guide/",
    "test": "NODE_ENV=production BABEL_ENV=production jest --config .jest.json"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "lodash.omit": "^4.5.0",
    "moment": "^2.14.1",
    "normalize.css": "^4.2.0",
    "popper.js": "1.0.6",
    "prop-types": "^15.5.8",
    "react-modal": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^15.4.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.14",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "ejs": "^2.5.2",
    "ejs-loader": "^0.3.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^19.0.2",
    "js-beautify": "^1.6.2",
    "jsdom": "^9.10.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.uniq": "^4.5.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "prismjs": "^1.4.1",
    "react": "^15.4.1",
    "react-docgen": "^2.12.1",
    "react-dom": "^15.4.1",
    "react-element-to-jsx-string": "^6.0.0",
    "react-router": "2.6.1",
    "react-test-renderer": "^15.4.1",
    "semantic-release": "^6.3.2",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "stylelint-order": "^0.4.1",
    "uuid": "^2.0.1",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  }
}
